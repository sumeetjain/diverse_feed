<div class="graph" data-graph-id="<%= graph.id %>">
  <div class="exhibit">
    <canvas id="graph_<%= graph.id %>" width="400" height="400"></canvas>
    <script>
      Chart.defaults.global.legend.position = "right";

      var element_<%= graph.id %> = document.getElementById("graph_<%= graph.id %>");
      var chart_<%= graph.id %> = new Chart(element_<%= graph.id %>, {
        type: 'pie',
        data: {
          labels: <%= graph.labels %>,
          datasets: [{
            label: '<%= graph.label %>',
            data: <%= graph.values %>
          }]
        },
        options: {
        }
      });
    </script>
  </div>
</div>
