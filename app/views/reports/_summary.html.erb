<div class="summary">
  <div class="container gridContainer">
    <div class="subject">
      <div class="subjectHeader">
        <img src="https://placeholdit.imgix.net/~text?txtsize=28&txt=160%C3%97160&w=160&h=160">

        <h2 class="summaryTitle">
          Diversity Report For
          <a href="#" class="subjectUsername"><%= report.subject %></a>
        </h2>
      </div>

      <ul class="metaInfo">
        <li><%= report.friends_count %></li>
        <li><%= report.friends_in_report_percentage %></li>
      </ul>
    </div>

    <div class="graphs">
        <% report.graphs.each do |graph| %>
          <%= graph.render %>
        <% end %>

      <ul class="demographicKeys nav">
        <% report.labels.each do |key| %>
          <li><a href="#"><%= key %></a></li>
        <% end %>
      </ul>

      <a href="#" class="btn">View Full Report</a>
    </div>
  </div>
</div>
